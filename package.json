{
  "name": "cdc-missing-manual",
  "private": true,
  "scripts": {
    "clean": "rimraf dist src/assets/css/styles.pref.css src/assets/css/styles.min.css",
    "build:css": "postcss src/assets/css/styles.css -o src/assets/css/styles.pref.css",
    "minify:css": "node scripts/minify-css.js src/assets/css/styles.pref.css src/assets/css/styles.min.css",
    "build": "npm run build:css && npm run minify:css && eleventy --config=eleventy.config.cjs",
    "smoke": "node scripts/smoke.js",
    "serve": "eleventy --config=eleventy.config.cjs --serve",
    "a11y": "pa11y-ci"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.20",
    "csso": "^5.0.5",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.5"
  }
}
