{
  "name": "cdc-missing-manual",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist src/assets/css/styles.pref.css src/assets/css/styles.min.css",
    "build:css": "postcss src/assets/css/styles.css -o src/assets/css/styles.pref.css",
    "minify:css": "node scripts/minify-css.js src/assets/css/styles.pref.css src/assets/css/styles.min.css",
    "build": "npm run build:css && npm run minify:css && eleventy --config=eleventy.config.cjs",
    "smoke:core": "node scripts/smoke.mjs && node scripts/visual-smoke.mjs",
    "smoke:a11y": "node scripts/run-pa11y.mjs",
    "smoke:perf": "node scripts/perf-budget.mjs",
    "smoke": "npm run smoke:core && npm run smoke:a11y && npm run smoke:perf",
    "serve": "eleventy --config=eleventy.config.cjs --serve",
    "a11y": "pa11y-ci --config pa11y-ci.config.cjs"
  },
  "dependencies": {
    "node-appwrite": "^13.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.20",
    "csso": "^5.0.5",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.5"
  }
}
